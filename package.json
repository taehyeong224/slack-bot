  "bugs": {
    "url": "https://github.com/taehyeong224/slack-bot/issues"
  },
  "homepage": "https://github.com/taehyeong224/slack-bot#readme",
  "dependencies": {
    "@slack/rtm-api": "^5.0.2",
    "@slack/web-api": "^5.1.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "core-js": "^2.6.9",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "node-schedule": "^1.3.2",
    "slackbots": "^1.0.2"
